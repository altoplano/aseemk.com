---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    
    <id>http://aseemk.com/blog/</id>
    <link href="http://aseemk.com/blog/" />
    <link href="http://aseemk.com/blog/atom.xml" rel="self" />
    <title>Aseem Kishore - Blog</title>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    
    <author>
        <name>Aseem Kishore</name>
        <email>aseem.kishore@gmail.com</email>
    </author>
    
    {% for post in site.posts %}
    <entry>
        <id>http://aseemk.com/{{ post.id | replace: '/posts/', 'blog/' }}</id>
        <link href="http://aseemk.com/{{ post.id | replace: '/posts/', 'blog/' }}" />
        <title>{{ post.title }}</title>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>